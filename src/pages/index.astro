  <CureoHero />
  <FacilitySelector />
  <PartnerReferences />
  <KnownFromSection />
  <CureoLeadingSection />
  <CureoUniqueSection />
  <CureoFlexSection />
  <CureoModulesSection />
  <VideoReferenceCase />
  <CureoFooter client:load />
</Layout>
